{
  "name": "shopstack",
  "version": "1.0.0",
  "description": "An ecommerce website built on Node.js and React.js",
  "main": "src/index.js",
  "engines": {
    "node": "11.9.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "install:all": "$(npm bin)/lerna exec \"npm install\" --stream",
    "dev": "$(npm bin)/lerna run dev --stream",
    "build": "$(npm bin)/lerna run build --stream",
    "start": "node ./packages/api/dist/index.js",
    "test": "$(npm bin)/lerna run test --stream",
    "heroku-postbuild": "npm run install:all && $(npm bin)/lerna run build --stream"
  },
  "keywords": [
    "node.js",
    "react.js",
    "redux.js",
    "redis"
  ],
  "author": "Ovie Okeh",
  "license": "Apache-2.0",
  "dependencies": {
    "lerna": "^3.14.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.1.0",
    "expect.js": "^0.3.1",
    "lerna": "^3.13.4",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1"
  }
}
