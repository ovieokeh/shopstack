{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "camelcase": 0,
    "no-console": 0,
    "no-undef": 0,
    "import/no-dynamic-require": 0,
    "import/no-extraneous-dependencies": 0,
    "consistent-return": 0,
    "arrow-body-style": 0,
    "import/prefer-default-export": 0,
    "no-param-reassign": 0,
    "no-prototype-builtins": 0,
    "no-await-in-loop": 0
  }
}
